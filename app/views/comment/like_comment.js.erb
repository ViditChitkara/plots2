<% str="##{@comment_id}-like-emojis button" %>
$("<%= str %>").css("display","none");
<% if @likes.has_key? "ThumbsUp" %>
  <% str="##{@comment_id}-emoji-button-thumbs-up" %>
  $("<%= str %>").css("display","flex");
  $("<%= str %> div").val("<%= @likes["ThumbsUp"] %>");
<% end %>
